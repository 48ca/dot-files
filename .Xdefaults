xterm*background: #050505
xterm*foreground: white
xterm*faceName: DejaVu Sans Mono:style=Book:size=12:antialias=true
xterm*faceSize: 12
xterm*font: 7x13

urxvt*background: #0A0909
urxvt*cursorBlink:     true
urxvt*cursorUnderline: true
urxvt*font:            xft:Fantasque Sans:size=12
urxvt*boldFont:        xft:Fantasque Sans:size=12:bold
urxvt*scrollBar:       false
urxvt.perl-ext-common: default,matcher
urxvt.url-launcher:    chromium
urxvt.matcher.button:  1
urxvt.internalBorder:  0
urxvt.lineSpace:       1
urxvt.letterSpace:     -3

